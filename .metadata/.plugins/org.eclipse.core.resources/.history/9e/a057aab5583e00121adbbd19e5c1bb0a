package view;

import static org.junit.Assert.*;

import model.StartWerte;

import org.junit.Test;

public class AnfangBildschirmTest {

	@Test
	public void StartNeueGUI() {
		StartWerte sw = new StartWerte();
		
		sw.setSteuerJahr(2012);
		sw.setButtonGehaltsSchein(1);
		sw.setButtonSpenden(0);
		sw.setButtonKrankheitsKosten(0);
		
		assertEquals(2012, 1, 0, 0); 
	}
	
}
